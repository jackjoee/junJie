class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int length = 0;
        for(int j = 0 ;j < nums.size() ;j++)
        {
            if(nums[j] != val)
            {
                nums[length] = nums[j];
                length++;
            }
        }
        return length;
    }
};
